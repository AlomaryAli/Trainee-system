@model IEnumerable<UserManagement02.Models.Supervisor>

@{
ViewData["Title"] = "Supervisors";
}

<h1>Supervisors</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">New Supervisor</a>
</p>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Full Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var sup in Model)
    {
    <tr>
        <td>@sup.FullName</td>
        <td>@sup.Email</td>
        <td>@sup.PhoneNumber</td>
        <td>
            <a asp-action="Edit"   asp-route-id="@sup.Id">Edit</a> |
            <a asp-action="Delete" asp-route-id="@sup.Id">Delete</a>
        </td>
    </tr>
    }
    </tbody>
</table>